@prefix ex: <http://example.org/masters/ev#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# Vocabulaire Turtle pour gérer les évaluations des étudiants
# Objet : Master — cours, évaluations, notes, évaluateurs, compétences
# Langue des labels et commentaires : français
#################################################################

#### Classes ####
ex:Student a rdfs:Class ;
    rdfs:label "Étudiant"@fr ;
    rdfs:comment "Représente un étudiant inscrit au Master."@fr .

ex:Instructor a rdfs:Class ;
    rdfs:label "Enseignant / Évaluateur"@fr ;
    rdfs:comment "Personne qui conçoit et/ou corrige des évaluations."@fr .

ex:Course a rdfs:Class ;
    rdfs:label "Cours"@fr ;
    rdfs:comment "Un cours du Master (UE) capable d'avoir plusieurs évaluations)."@fr .

ex:Module a rdfs:Class ;
    rdfs:label "Module"@fr ;
    rdfs:comment "Un regroupement de cours (optionnel)."@fr .

ex:Assessment a rdfs:Class ;
    rdfs:label "Évaluation"@fr ;
    rdfs:comment "Générique : examen, devoir, projet, contrôle continu..."@fr .

ex:Exam a rdfs:Class ;
    rdfs:subClassOf ex:Assessment ;
    rdfs:label "Examen"@fr .

ex:Assignment a rdfs:Class ;
    rdfs:subClassOf ex:Assessment ;
    rdfs:label "Devoir / Travail dirigé / Projet"@fr .

ex:ContinuousAssessment a rdfs:Class ;
    rdfs:subClassOf ex:Assessment ;
    rdfs:label "Évaluation continue"@fr .

ex:Grade a rdfs:Class ;
    rdfs:label "Note / Résultat final"@fr ;
    rdfs:comment "Représente la note (chiffre, mention) attribuée pour un cours ou une évaluation."@fr .

ex:LearningOutcome a rdfs:Class ;
    rdfs:label "Compétence / Objectif d'apprentissage"@fr ;
    rdfs:comment "Compétence visée par le cours et éventuellement évaluée."@fr .

ex:Semester a rdfs:Class ;
    rdfs:label "Semestre"@fr ;
    rdfs:comment "Période académique (ex : S1, S2)."@fr .

#### Properties ####
# Relations étudiants / inscriptions
ex:enrolledIn a rdf:Property ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Course ;
    rdfs:label "inscrit à"@fr ;
    rdfs:comment "Relie un étudiant à un cours où il est inscrit."@fr .

ex:takesModule a rdf:Property ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Module ;
    rdfs:label "participe au module"@fr .

# Structure des cours
ex:hasAssessment a rdf:Property ;
    rdfs:domain ex:Course ;
    rdfs:range ex:Assessment ;
    rdfs:label "a pour évaluation"@fr ;
    rdfs:comment "Relie un cours à ses évaluations (plusieurs possible)."@fr .

ex:belongsToCourse a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range ex:Course ;
    rdfs:label "appartient au cours"@fr .

ex:assessesLearningOutcome a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range ex:LearningOutcome ;
    rdfs:label "évalue la compétence"@fr ;
    rdfs:comment "Relie une évaluation à une ou plusieurs compétences visées."@fr .

# Détails d'une évaluation
ex:assessmentTitle a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range xsd:string ;
    rdfs:label "titre de l'évaluation"@fr .

ex:assessmentDate a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range xsd:dateTime ;
    rdfs:label "date de l'évaluation"@fr .

ex:dueDate a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range xsd:dateTime ;
    rdfs:label "date de rendu / échéance"@fr .

ex:weight a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range xsd:decimal ;
    rdfs:label "pondération"@fr ;
    rdfs:comment "Valeur entre 0 et 1 représentant le poids de l'évaluation dans la note finale du cours."@fr .

ex:evaluatedBy a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range ex:Instructor ;
    rdfs:label "corrigé par"@fr .

ex:dateEvaluated a rdf:Property ;
    rdfs:domain ex:Assessment ;
    rdfs:range xsd:dateTime ;
    rdfs:label "date de correction"@fr .

ex:score a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range xsd:decimal ;
    rdfs:label "score numérique"@fr ;
    rdfs:comment "Valeur numérique (ex: 0-20, 0-100) associée à une note."@fr .

ex:gradeLetter a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range xsd:string ;
    rdfs:label "mention / lettre"@fr ;
    rdfs:comment "Ex: A/B/C ou mentions (Très Bien, Bien...)."@fr .

ex:gradeFor a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range ex:Course ;
    rdfs:label "note pour le cours"@fr .

ex:forAssessment a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range ex:Assessment ;
    rdfs:label "note pour l'évaluation"@fr .

ex:passed a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range xsd:boolean ;
    rdfs:label "validé"@fr ;
    rdfs:comment "true si la note est suffisante pour valider l'évaluation ou le cours."@fr .

ex:feedback a rdf:Property ;
    rdfs:domain ex:Grade ;
    rdfs:range xsd:string ;
    rdfs:label "rétroaction"@fr ;
    rdfs:comment "Commentaire du correcteur pour l'étudiant."@fr .

# Liens temporels / organisation
ex:offeredInSemester a rdf:Property ;
    rdfs:domain ex:Course ;
    rdfs:range ex:Semester ;
    rdfs:label "proposé au semestre"@fr .

# Autres propriétés utiles
ex:hasLearningOutcome a rdf:Property ;
    rdfs:domain ex:Course ;
    rdfs:range ex:LearningOutcome ;
    rdfs:label "vise la compétence"@fr .

ex:email a rdf:Property ;
    rdfs:domain [ owl:unionOf (ex:Student ex:Instructor) ] ;
    rdfs:range xsd:string ;
    rdfs:label "adresse email"@fr .

ex:studentId a rdf:Property ;
    rdfs:domain ex:Student ;
    rdfs:range xsd:string ;
    rdfs:label "identifiant étudiant"@fr .

ex:instructorId a rdf:Property ;
    rdfs:domain ex:Instructor ;
    rdfs:range xsd:string ;
    rdfs:label "identifiant enseignant"@fr .

#################################################################
# Quelques individus / exemple d'utilisation (modèle minimal)
#################################################################

# Semestre
ex:S2025 a ex:Semester ;
    rdfs:label "Semestre 2025"@fr .

# Cours
ex:CS101 a ex:Course ;
    rdfs:label "Fondamentaux du Web"@fr ;
    ex:offeredInSemester ex:S2025 ;
    ex:hasLearningOutcome ex:LO1 , ex:LO2 ;
    ex:hasAssessment ex:CS101_Exam1 , ex:CS101_Project1 .

# Compétences
ex:LO1 a ex:LearningOutcome ;
    rdfs:label "Savoir structurer une page web (HTML/CSS)"@fr .

ex:LO2 a ex:LearningOutcome ;
    rdfs:label "Savoir réaliser une interaction cliente simple (JS)"@fr .

# Enseignant
ex:enseignant_jdupont a ex:Instructor ;
    rdfs:label "Jean Dupont"@fr ;
    ex:instructorId "JDUP2025" .

# Étudiant
ex:etudiant_amartin a ex:Student ;
    rdfs:label "Alice Martin"@fr ;
    ex:studentId "AM2025" ;
    ex:enrolledIn ex:CS101 .

# Évaluations
ex:CS101_Exam1 a ex:Exam ;
    ex:assessmentTitle "Examen final" ;
    ex:assessmentDate "2025-06-15T09:00:00"^^xsd:dateTime ;
    ex:weight "0.6"^^xsd:decimal ;
    ex:evaluatedBy ex:enseignant_jdupont ;
    ex:assessesLearningOutcome ex:LO1 , ex:LO2 ;
    rdfs:label "Examen final - CS101"@fr .

ex:CS101_Project1 a ex:Assignment ;
    ex:assessmentTitle "Projet Web" ;
    ex:dueDate "2025-05-20T23:59:00"^^xsd:dateTime ;
    ex:weight "0.4"^^xsd:decimal ;
    ex:evaluatedBy ex:enseignant_jdupont ;
    ex:assessesLearningOutcome ex:LO2 ;
    rdfs:label "Projet - CS101"@fr .

# Notes
ex:grade_AM_Exam1 a ex:Grade ;
    ex:forAssessment ex:CS101_Exam1 ;
    ex:score "15.5"^^xsd:decimal ;
    ex:gradeFor ex:CS101 ;
    ex:passed true ;
    ex:feedback "Bonne maîtrise des concepts. Quelques erreurs sur la synthaxe JS."@fr .

ex:grade_AM_Project1 a ex:Grade ;
    ex:forAssessment ex:CS101_Project1 ;
    ex:score "17"^^xsd:decimal ;
    ex:gradeFor ex:CS101 ;
    ex:passed true ;
    ex:feedback "Projet complet et bien documenté."@fr .

# Calcul simple : note finale (exemple non inféré automatiquement)
# (15.5 * 0.6) + (17 * 0.4) = 16.06
ex:finalGrade_AM_CS101 a ex:Grade ;
    ex:gradeFor ex:CS101 ;
    ex:score "16.06"^^xsd:decimal ;
    ex:passed true ;
    rdfs:label "Note finale Alice Martin - CS101"@fr .

#################################################################
# Remarques :
# - Ce vocabulaire est volontairement simple et extensible.
# - On peut ajouter des classes/propriétés pour : groupes d'étudiants,
#   sessions de rattrapage, barèmes détaillés, anonymisation, historique
#   des corrections, validations administratives, etc.
# - Pour automatisation (ex : calcul automatique de la note finale),
#   utiliser des règles SWRL ou calculs applicatifs en dehors du graphe RDF.
#################################################################
